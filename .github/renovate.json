{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "labels": [
    "eslint-config-e7npm"
  ],
  "extends": [
    "github>andreigec/renovate"
  ],
  "rangeStrategy": "replace",
  "lockFileMaintenance": {
    "enabled": true,
    "recreateWhen": "always",
    "rebaseStalePrs": true,
    "schedule": [
      "every 2 weeks on monday"
    ]
  }
}